<?xml version="1.0" encoding="UTF-8" ?>
<parameters>

	<parameter name="randomSeed" displayName="Default Random Seed" type="int" defaultValue="__NULL__" />

	<parameter name="ticksPerMonth" displayName="ticksPerMonth" type="java.lang.Integer" 
						defaultValue="30" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									values="1 30 60 90 "
						
		/>
		
	<parameter name="startDate" displayName="startDate" type="java.lang.String" 
						defaultValue="01/01/2012" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$StringStringConverter"
		/>
		
		<parameter name="nearbyNeighbours" displayName="Nearby Neighbours Radius" type="int" 
						defaultValue="2" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$IntConverter"
									
		/>
		
	<parameter name="pRecycles" displayName="Probability of Being a recycler" type="double" 
						defaultValue="0.8" 
						isReadOnly="false" 
						converter="repast.simphony.parameter.StringConverterFactory$DoubleConverter"
									
		/>
		
</parameters>
